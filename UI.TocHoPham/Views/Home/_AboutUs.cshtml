@model AboutUs

<div class="footer">
    <h2 style="font-size: 25px; color: white;padding-top: 10px">@Model.Title</h2>
    <div class="col-md-6 form-contact">
        <div class="w3layoutscontaineragileits" style="width: 90%">
            <form action="#" method="post" class="contact-input-form">
                <input type="text" Name="Username" placeholder="Tên" required="">
                <input type="email" Name="Username" placeholder="email" required="">
                <input type="text" Name="Username" placeholder="Số điện thoại"
                       required="">
                <textarea name="" value="" cols="30" rows="5"
                          placeholder="Nội dung" required></textarea>
                <div class="aitssendbuttonw3ls">
                    <input class="button" type="submit" value="Gửi">
                </div>
            </form>
        </div>
    </div>
    <div class="col-md-6" style="padding-bottom: 20px">
        @Html.Raw(Model.BodyHtml)
    </div>
    <div class="col-sm-12">
        <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d7838.934772993494!2d106.679714!3d10.77547!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x6d6571e98017fe54!2zQ8O0bmcgdHkgVE5ISCBBIFNv4bq7biBUb8OgbiBD4bqndQ!5e0!3m2!1svi!2sus!4v1514690704118"
                style="border: 0px none; pointer-events: none;" allowfullscreen=""
                width="100%" height="450" frameborder="0"></iframe>
    </div>
</div>
